{
  "name": "repairs-management",
  "scripts": {
  },
  "env": {
    "LANG": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_LOG_TO_STDOUT": {
      "required": true
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true
    },
    "RAILS_MASTER_KEY": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    },
    "AAD_CLIENT_ID": {
      "required": true
    },
    "AAD_TENANT": {
      "required": true
    },
    "HACKNEY_REPAIRS_API_BASE_URL": {
      "required": true
    },
    "HEROKU_APP_NAME": {
      "required": true
    },
    "X_API_KEY": {
      "required": true
    }
