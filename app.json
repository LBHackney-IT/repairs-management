  "formation": {
    "web": {
      "quantity": 1,
      "size": "hobby"
    }
  },
  "addons": [
    {
      "plan": "graphenedb",
      "options": {
        "version": "3.3.6"
      }
    },
    {
      "plan": "heroku-postgresql"
    },
    {
      "plan": "heroku-redis"
    }
